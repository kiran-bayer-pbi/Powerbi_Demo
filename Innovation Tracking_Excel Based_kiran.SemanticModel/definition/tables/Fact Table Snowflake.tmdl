				    Source = Value.NativeQuery(Snowflake.Databases("BAY-CHDAAPRD.snowflakecomputing.com","ZDATACORE_DP_BRO_REPORTING",[Implementation="2.0", Role="CH.DAA.BROZONE.DATAENGINEER.DEV"]){[Name="ZDATACORE_DP_BRO"]}[Data], "---ACTUALS AND LPPM VALUES--29.09.2025#(lf)#(lf)SELECT DATE,COUNTRY_MOD,REGION,CLUSTER,COUNTRY, LAUNCH_DATE,BRAND,""NPD Name"",""NPD Number"",LPPM_NAME,LPPM_NUMBER,""LPPM Innovation Type"",SUM(""LPPM Value BUD"") LPPM_Value,#(lf)       round(SUM(""Actual Value BUD"" ),2) AS Actuals,category, sum(""Budget Value BUD"") Budget_Value,#(lf)       case when ""Innovation Type"" = 'Rollouts' then concat(""NPD Name"",'(#)')#(lf)            when ""Innovation Type"" <> 'Rollouts' then concat(""NPD Name"",' (',brand,')') end as NPD_Brand,#(lf)      case  when ""Innovation Type"" = 'Rollouts' then concat('https://synaps-apps.intranet.cnb/NPDManagement/npds/','Roll-outs(#)','#/detailsContent')#(lf)            when ""Innovation Type"" <> 'Rollouts' then concat('https://synaps-apps.intranet.cnb/NPDManagement/npds/',""NPD Number"",'#/detailsContent') end as Innova_Link#(lf)FROM ZDATACORE_DP_BRO.LX.V_LX_LEGACY_PREVIEW#(lf)--where DATE >= '2025-05-01' and DATE <= '2025-05-31'#(lf)GROUP BY DATE,COUNTRY_MOD,category,REGION,CLUSTER,COUNTRY, LAUNCH_DATE,BRAND,""NPD Name"",""NPD Number"",LPPM_NAME,LPPM_NUMBER,""LPPM Innovation Type"",""Innovation Type""", null, [EnableFolding=true])
				in
				    Source

